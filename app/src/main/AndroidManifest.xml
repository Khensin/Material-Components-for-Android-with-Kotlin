<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="edu.stts.materialcomponents">

    <application
            android:allowBackup="false"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:replace="android:allowBackup">
        <activity android:name="edu.stts.materialcomponents.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data
                        android:scheme="http"
                        android:host="stts.edu"
                        android:pathPattern="/material-components"/>
            </intent-filter>
        </activity>
        <activity android:name="edu.stts.materialcomponents.activity.MainActivity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu1Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu2Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu3Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu4Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu5Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu6Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu7Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu8Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu9Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu10Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu11Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu12Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu13Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu14Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu15Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu16Activity"/>
        <activity android:name="edu.stts.materialcomponents.activity.Menu17Activity"/>
    </application>

</manifest>